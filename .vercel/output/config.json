{
	"version": 3,
	"routes": [
		{
			"src": ".*",
			"continue": true,
			"transforms": [
				{
					"type": "request.query",
					"op": "delete",
					"target": {
						"key": "__pathname"
					}
				}
			]
		},
		{
			"src": "/_app/immutable/.+",
			"headers": {
				"cache-control": "public, immutable, max-age=31536000"
			}
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/_app/immutable/.+",
			"status": 404,
			"continue": false
		},
		{
			"src": "/.*",
			"dest": "/![-]/catchall"
		}
	],
	"overrides": {
		"index.html": {
			"path": ""
		}
	}
}